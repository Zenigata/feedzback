{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": "off",
        "new-cap": "off",
        "max-len": "off"
    },
    "overrides": [
        {
          "files": [
            "*.html"
          ],
          "extends": [
            "plugin:@angular-eslint/template/recommended",
            "plugin:cypress/recommended"
          ],
          "plugins": [
            "html",
            "eslint-plugin-prettier"
          ],
          "rules": {
            "no-trailing-spaces": "off",
            "indent":"off" 
          }
        }
      ]
}
