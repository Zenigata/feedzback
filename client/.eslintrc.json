{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "globals": {
        "cy": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "jest": true,
        "expect": true
      },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-prettier"
    ],
    "rules": {
        "no-useless-constructor":"off",
        "prettier/prettier": "error"
    }
}
